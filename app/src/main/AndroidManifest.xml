<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assessment.it.itskillsassessment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExamActivity"
            android:label="@string/title_activity_exam"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SelectActivity"
            android:label="@string/title_activity_select"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DifficultySelectActivity"
            android:label="@string/title_activity_difficulty_select"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegisterUserActivity"
            android:label="@string/title_activity_register_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AllResultsActivity"
            android:label="@string/title_activity_all_results"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>